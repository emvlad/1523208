        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView_pourGagner"
        android:layout_width="89dp"
        android:layout_height="23dp"
        android:layout_marginBottom="244dp"
        android:layout_marginStart="40dp"
